# AI增强配置示例文件
# 请将此文件重命名为 .env 并填入您的API密钥

# OpenAI配置
OPENAI_API_KEY=your_openai_api_key_here
# OPENAI_API_BASE=https://api.openai.com/v1  # 可选，自定义API基址

# 通义千问配置
QWEN_API_KEY=your_qwen_api_key_here
# QWEN_API_BASE=https://dashscope.aliyuncs.com/api/v1/services/aigc/text-generation/generation

# ChatGLM配置
CHATGLM_API_KEY=your_chatglm_api_key_here
# CHATGLM_API_BASE=https://open.bigmodel.cn/api/paas/v4/chat/completions

# 本地模型配置（如Ollama）
# LOCAL_API_BASE=http://localhost:11434/api/generate

# 默认AI增强设置
DEFAULT_AI_PROVIDER=openai
DEFAULT_AI_MODEL=gpt-3.5-turbo
DEFAULT_ENHANCEMENT_TYPE=comprehensive

# AI增强功能开关
ENABLE_AI_ENHANCEMENT=true